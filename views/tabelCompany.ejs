<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <title>Tabel Company</title>
</head>
<body>
    <!-- NAVBAR -->
    <%- include('./navbarAdmin') %>
    
    <h1 style="text-align: center;">Tabel Company</h1>
    <a href="/admins/addCompanys"> Add Company</a>

    <br>
    <br>
    <table class="table table-stripped">
        <tr>
            <th>No</th>
            <th>name Company</th>
            <th>Address</th>
            <th>Phone Number</th>
            <th>Action</th>
        </tr>
            <% tabelCompany.forEach((el,i) => { %>
        <tr>
            <td> <%=(i+1)%> </td>
            <td> <%=el.name%> </td>
            <td> <%=el.address%> </td>
            <td> <%=el.phoneNumber%> </td>
            <td> <a href="/admins/<%=el.id%>/editCompanys"><button class="btn btn-primary">Edit</button></a></td>
        </tr> 
            <% }) %>
        
    </table>
</body>
</html>