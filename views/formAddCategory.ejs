<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Add Category</title>
</head>
<body>
    <a href="/">Home</a>
    <a href="/admins">home Admin</a>
    <a href="/users">home Users</a>
    <br>
    <br>
    <h1>Tabel Category</h1>
    <a href="/users/addCategory"> Add Admin</a>
    <br>
    <br>
    <table border="1">
        <tr>
            <th>No</th>
            <th>name Category</th>
            <th colspan="2">Action</th>
        </tr>
        
            <% tabelCategory.forEach((el,i) => { %>
        <tr>
            <td> <%=(i+1)%> </td>
            <td> <%=el.username%> </td>
            <td> <a href="/admins/<%=el.id%>/edit">  <button>  Edit  </button> </a> </td>
            <td> <a href="/admins/<%=el.id%>/delete"> <button> Delete </button> </a> </td>
    </tr>
            <% }) %>
        
    </table>
</body>
</html>