<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
    <!-- NAVBAR -->
    <%- include('./navbarAdmin') %>

    <h1 style="text-align: center;">Tabel User Admin</h1>
    <a href="/admins/add"> Add Admin</a>
    <br>
    <br>
    <table class="table table-striped">
        <tr>
            <th>No</th>
            <th>Username</th>
            <th>Password</th>
            <th>Role</th>
            <th colspan="2">Action</th>
        </tr>
        
            <% tabelUsers.forEach((el,i) => { %>
        <tr>
            <td> <%=(i+1)%> </td>
            <td> <%=el.username%> </td>
            <td> <%=el.password%> </td>
            <td> <%=el.role%> </td>
            <td> <a href="/admins/<%=el.id%>/edit">  <button class="btn btn-primary">  Edit  </button> </a> </td>
            <td> <a href="/admins/<%=el.id%>/delete"> <button class="btn btn-danger"> Delete </button> </a> </td>
    </tr>
            <% }) %>
        
    </table>
</body>
</html>